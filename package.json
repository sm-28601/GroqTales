{
  "name": "groqtales",
  "version": "1.2.8",
  "private": true,
  "description": "Enterprise-grade AI-powered Web3 storytelling platform with NFT integration",
  "author": "GroqTales Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Drago-03/GroqTales.git"
  },
  "keywords": [
    "ai",
    "blockchain",
    "nft",
    "storytelling",
    "web3",
    "nextjs",
    "react",
    "groq"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "concurrently \"next dev\" \"nodemon server/backend.js\"",
    "build": "cross-env NEXT_PUBLIC_BUILD_MODE=true next build",
    "start": "concurrently \"next start\" \"node server/backend.js\"",
    "vercel-build": "npm run prepare-vercel && npm run build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "setup-db": "ts-node scripts/setup-db.ts",
    "check-env": "node scripts/check-env.js",
    "prepare-vercel": "node scripts/prepare-vercel.js",
    "predeploy": "npm run type-check && npm run lint && npm run test",
    "debug-build": "node scripts/debug-build.js",
    "security-audit": "npm audit --audit-level moderate",
    "deps-update": "npm update && npm audit fix",
    "clean": "rm -rf .next dist coverage",
    "pre-commit": "lint-staged",
    "build:backend": "echo 'Backend build completed'",
    "start:backend": "node server/backend.js",
    "build:sdk": "echo 'SDK build completed'",
    "start:sdk": "node server/sdk-server.js",
    "start:worker": "node server/worker.js",
    "backend:dev": "nodemon server/backend.js",
    "backend:test": "node server/backend.js"
  },
